    swrc:category "Semantic Web In Use Track Paper" ;
    swrc:link_open_access <https://github.com/lidingpku/iswc2014/raw/master/paper/87960001-camo-integration-of-linked-open-data-for-multimedia-metadata-enrichment.pdf> ;
