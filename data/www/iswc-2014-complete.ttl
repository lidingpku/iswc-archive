    swrc:listAuthor "A. Elizabeth Cano,Yulan He and Harith Alani" ;
    swrc:pages "337-352" ;
    swrc:url <https://github.com/lidingpku/iswc2014/raw/master/paper/87970337-stretching-the-life-of-twitter-classifiers-with-time-stamped-semantic-graphs.pdf> ;
