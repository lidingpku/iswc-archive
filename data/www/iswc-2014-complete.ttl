    swrc:listAuthor "Ricardo Usbeck,Axel-Cyrille Ngonga Ngomo,Michael Röder,Daniel Gerber,Sandro Athaide Coelho,Sören Auer and Andreas Both" ;
    swrc:pages "449-463" ;
