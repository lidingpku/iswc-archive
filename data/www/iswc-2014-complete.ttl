    swrc:category "Replication, Benchmark, Data and Software Track Paper" ;
    swrc:link_open_access <https://github.com/lidingpku/iswc2014/raw/master/paper/87960065-web-scale-extension-of-rdf-knowledge-bases-from-templated-websites.pdf> ;
    swrc:listAuthor "Lorenz Bühmann,Ricardo Usbeck,Axel-Cyrille Ngonga Ngomo,Muhammad Saleem,Andreas Both,Valter Crescenzi,Paolo Merialdo and Disheng Qiu" ;
    swrc:pages "65-80" ;
