    swrc:listAuthor "Man Zhu,Zhiqiang Gao and Zhibin Quan" ;
    swrc:pages "369-384" ;
    swrc:url <https://github.com/lidingpku/iswc2014/raw/master/paper/87960369-noisy-type-assertion-detection-in-semantic-datasets.pdf> ;
