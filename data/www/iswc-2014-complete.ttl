    swrc:category "Research Track Paper" ;
    swrc:link_open_access <https://github.com/lidingpku/iswc2014/raw/master/paper/87970145-pushing-the-boundaries-of-tractable-ontology-reasoning.pdf> ;
    swrc:listAuthor "David Carral,Cristina Feier,Bernardo Cuenca Grau,Pascal Hitzler and Ian Horrocks" ;
    swrc:pages "145-160" ;
    swrc:url <https://github.com/lidingpku/iswc2014/raw/master/paper/87970145-pushing-the-boundaries-of-tractable-ontology-reasoning.pdf> ;
