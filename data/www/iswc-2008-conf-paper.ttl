    swrc:year "2008" ;
    foaf:maker <http://data.semanticweb.org/person/benjamin-nowack> .

<http://data.semanticweb.org/conference/iswc/2008/poster-demo-proceedings/paper-56> a swrc:InProceedings ;
    rdfs:label "EUREEKA: Deepening the Semantic Web by More Efficient Emergent Knowledge Representation and Processing" ;
    swc:isPartOf <http://data.semanticweb.org/conference/iswc/2008/poster-demo-proceedings> ;
    dc:title "EUREEKA: Deepening the Semantic Web by More Efficient Emergent Knowledge Representation and Processing" ;
