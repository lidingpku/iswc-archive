    swrc:listAuthor "Kay-Uwe Schmidt,Daniel Oberle and Klaus Deissner" ;
    swrc:listKeyword "Enterprise Search,Flogic,Ontologies,Personalization,SAP" ;
    swrc:url <http://ceur-ws.org/Vol-401/iswc2008pd_submission_22.pdf> ;
    swrc:year "2008" ;
    foaf:maker <http://data.semanticweb.org/person/daniel-oberle>,
        <http://data.semanticweb.org/person/kay-uwe-schmidt>,
        <http://data.semanticweb.org/person/klaus-deissner> .

<http://data.semanticweb.org/conference/iswc/2008/poster-demo-proceedings/paper-26> a swrc:InProceedings ;
    rdfs:label "Reusing Knowledge from the Semantic Web with the Watson Plugin" ;
    swc:isPartOf <http://data.semanticweb.org/conference/iswc/2008/poster-demo-proceedings> ;
    dc:title "Reusing Knowledge from the Semantic Web with the Watson Plugin" ;
    dcterms:title "Reusing Knowledge from the Semantic Web with the Watson Plugin" ;
    bibo:authorList <http://data.semanticweb.org/conference/iswc/2008/poster-demo-proceedings/paper-26/author_list> ;
    swrc:abstract "The recent growth of the Semantic Web increases the amount of ontologies that can be reused, but also makes more difficult the tasks of finding, selecting and integrating reusable knowledge for ontology engineering. For this reason, we developed the Watson plugin, a tool which aims to facilitate large scale knowledge reuse by extending an ontology editor with the features of the Watson Semantic Web search engine. With this plugin, it is possible to discover, inspect and reuse ontology statements originating from various online ontologies directly in the ontology engineering environment." ;
    swrc:author <http://data.semanticweb.org/person/enrico-motta>,
        <http://data.semanticweb.org/person/marta-sabou>,
        <http://data.semanticweb.org/person/mathieu-daquin> ;
