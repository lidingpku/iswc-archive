<http://data.semanticweb.org/conference/iswc/2008/proceedings/paper-24> a swrc:InProceedings ;
    rdfs:label "Extracting Semantic Constraint from Description Text for Semantic Web Service Discovery" ;
    swc:isPartOf <http://data.semanticweb.org/conference/iswc/2008/proceedings> ;
    dc:title "Extracting Semantic Constraint from Description Text for Semantic Web Service Discovery" ;
