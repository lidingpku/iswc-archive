<http://data.semanticweb.org/conference/iswc/2008/poster-demo-proceedings/paper-32> a swrc:InProceedings ;
    rdfs:label "ACTIVE - Enabling the Knowledge-Powered Enterprise" ;
    swc:isPartOf <http://data.semanticweb.org/conference/iswc/2008/poster-demo-proceedings> ;
