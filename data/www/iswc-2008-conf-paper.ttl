    swrc:link_publisher <http://dx.doi.org/10.1007/978-3-540-88564-1_7> ;
    swrc:listAuthor "Eyal Oren,Christophe Guéret and Stefan Schlobach" ;
    swrc:listKeyword "SPARQL,approximate reasoning,bloom filters,evolutionary algorithms,query answering" ;
    swrc:pages "98-113" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-540-88564-1_7>,
