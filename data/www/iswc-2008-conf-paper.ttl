    swrc:year "2008" ;
    foaf:maker <http://data.semanticweb.org/person/malte-kiesel>,
        <http://data.semanticweb.org/person/sven-schwarz> .

<http://data.semanticweb.org/conference/iswc/2008/poster-demo-proceedings/paper-37> a swrc:InProceedings ;
