    swrc:link_publisher <http://dx.doi.org/10.1007/978-3-540-88564-1_18> ;
    swrc:listAuthor "Georgios Meditskos and Nick Bassiliades" ;
    swrc:listKeyword "DL Reasoning,Entailment Rules,Hybrid and Homogeneous Systems,Rule Engines,Rule-based OWL Reasoning" ;
    swrc:pages "277-292" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-540-88564-1_18>,
        <https://github.com/lidingpku/iswc-archive/raw/master/paper/iswc-2008/53180273.pdf> ;
    swrc:year "2008" ;
