    swrc:listAuthor "Michael Schneider,Felix Kratzer and Kai Mainzer" ;
    swrc:listKeyword "Linked Data,Second Life,Tagging" ;
    swrc:url <http://ceur-ws.org/Vol-401/iswc2008pd_submission_76.pdf> ;
    swrc:year "2008" ;
    foaf:maker <http://data.semanticweb.org/person/felix-kratzer>,
        <http://data.semanticweb.org/person/kai-mainzer>,
        <http://data.semanticweb.org/person/michael-schneider> .

<http://data.semanticweb.org/conference/iswc/2008/poster-demo-proceedings/paper-76> a swrc:InProceedings ;
