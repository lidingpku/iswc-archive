    swrc:link_publisher <http://dx.doi.org/10.1007/978-3-540-88564-1_14> ;
    swrc:listAuthor "Pavel Klinov and Bijan Parsia" ;
    swrc:listKeyword "Description Logic,Evaluation,Optimization,Probabilistic Reasoning" ;
    swrc:pages "213-228" ;
