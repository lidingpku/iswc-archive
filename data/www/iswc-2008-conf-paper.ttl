    swrc:listAuthor "Laurent Mazuel and Nicolas Sabouret" ;
    swrc:listKeyword "Evaluation,Heterogeneous relations,Ontology,Semantic relatedness" ;
    swrc:pages "681-694" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-540-88564-1_43>,
        <https://github.com/lidingpku/iswc-archive/raw/master/paper/iswc-2008/53180670.pdf> ;
