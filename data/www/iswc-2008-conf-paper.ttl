    swrc:listAuthor "Maciej Janik and Krys Kochut" ;
    swrc:listKeyword "Ontology-based text categorization,Semantic Web" ;
    swrc:url <http://ceur-ws.org/Vol-401/iswc2008pd_submission_75.pdf> ;
