    swrc:url <https://github.com/lidingpku/iswc-archive/raw/master/paper/iswc-2013/82190417.pdf> ;
    swrc:year "2013" ;
    foaf:maker <http://data.semanticweb.org/person/tom-de-nies> .

<http://data.semanticweb.org/conference/iswc/2013/proceedings-2/paper-29> a swrc:InProceedings ;
    rdfs:label "Utilising Provenance to Enhance Social Computation" ;
    swc:isPartOf <http://data.semanticweb.org/conference/iswc/2013/proceedings-2> ;
    dc:title "Utilising Provenance to Enhance Social Computation" ;
    dcterms:title "Utilising Provenance to Enhance Social Computation" ;
