<http://data.semanticweb.org/conference/iswc/2013/poster-demo-proceedings/paper-69> a swrc:InProceedings ;
    rdfs:label "SemantEco Annotator" ;
    swc:isPartOf <http://data.semanticweb.org/conference/iswc/2013/poster-demo-proceedings> ;
