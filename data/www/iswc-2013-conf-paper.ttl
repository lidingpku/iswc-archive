    foaf:maker <http://data.semanticweb.org/person/aidan-hogan>,
        <http://data.semanticweb.org/person/alessandra-mileo>,
        <http://data.semanticweb.org/person/emir-munoz> .

<http://data.semanticweb.org/conference/iswc/2013/poster-demo-proceedings/paper-36> a swrc:InProceedings ;
    rdfs:label "KbQAS: A Knowledge-based QA System" ;
    swc:isPartOf <http://data.semanticweb.org/conference/iswc/2013/poster-demo-proceedings> ;
    dc:title "KbQAS: A Knowledge-based QA System" ;
    dcterms:title "KbQAS: A Knowledge-based QA System" ;
