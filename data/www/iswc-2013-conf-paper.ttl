    swrc:listAuthor "Veli Bicer,Thanh Tran and Yongtao Ma" ;
    swrc:pages "1-16" ;
    swrc:url <https://github.com/lidingpku/iswc-archive/raw/master/paper/iswc-2013/82180001.pdf> ;
    swrc:year "2013" ;
    foaf:maker <http://data.semanticweb.org/person/thanh-tran>,
        <http://data.semanticweb.org/person/veli-bicer>,
