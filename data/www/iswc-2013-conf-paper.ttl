    swrc:listAuthor "Amrapali Zaveri,Joao Ricardo Nickenig Vissoci,Cinzia Daraio and Ricardo Pietrobon" ;
    swrc:pages "241-256" ;
    swrc:url <https://github.com/lidingpku/iswc-archive/raw/master/paper/iswc-2013/82190241.pdf> ;
    swrc:year "2013" ;
