    swrc:pages "161-176" ;
    swrc:url <https://github.com/lidingpku/iswc-archive/raw/master/paper/iswc-2013/82180161.pdf> ;
    swrc:year "2013" ;
    foaf:maker <http://data.semanticweb.org/person/daniel-m-herzig>,
        <http://data.semanticweb.org/person/peter-mika>,
