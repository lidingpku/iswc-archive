    swrc:category "Research Track Paper" ;
    swrc:link_open_access <https://github.com/lidingpku/iswc-archive/raw/master/paper/iswc-2013/82180625.pdf> ;
    swrc:listAuthor "Alberto Tonon,Michele Catasta,Gianluca Demartini,Philippe Cudr-Mauroux and Karl Aberer" ;
    swrc:pages "625-640" ;
