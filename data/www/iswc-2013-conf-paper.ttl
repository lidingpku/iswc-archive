        <http://data.semanticweb.org/person/ulrike-sattler> ;
    swrc:category "Research Track Paper" ;
    swrc:link_open_access <https://github.com/lidingpku/iswc-archive/raw/master/paper/iswc-2013/82180081.pdf> ;
