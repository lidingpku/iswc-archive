    swrc:listAuthor "Takahisa Fujino and Naoki Fukuta" ;
    swrc:listKeyword "SPARQL,heterogeneous ontology,query translation" ;
    swrc:url <http://ceur-ws.org/Vol-914/paper_57.pdf> ;
    swrc:year "2012" ;
    foaf:maker <http://data.semanticweb.org/person/naoki-fukuta>,
        <http://data.semanticweb.org/person/takahisa-fujino> .

<http://data.semanticweb.org/conference/iswc/2012/poster-demo-proceedings/paper-30> a swrc:InProceedings ;
