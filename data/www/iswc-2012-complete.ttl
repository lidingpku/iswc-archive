    swrc:link_publisher <http://dx.doi.org/10.1007/978-3-642-35176-1_7> ;
    swrc:listAuthor "Rafael S. Gonçalves,Bijan Parsia and Ulrike Sattler" ;
    swrc:listKeyword "Description Logics,NCI Thesaurus,OWL Ontologies,Semantic Diff" ;
    swrc:pages "99-115" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-642-35176-1_7>,
        <http://iswc2012.semanticweb.org/sites/default/files/76490097.pdf> ;
    swrc:year "2012" ;
    foaf:maker <http://data.semanticweb.org/person/bijan-parsia>,
