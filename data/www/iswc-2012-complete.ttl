    swrc:listAuthor "Jürgen Umbrich,Marcel Karnstedt,Aidan Hogan and Josiane Xavier Parreira" ;
    swrc:listKeyword "Hybrid SPARQL,Linked Data,Live querying,SPARQL" ;
    swrc:pages "608-624" ;
