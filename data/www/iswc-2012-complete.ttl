    swrc:listAuthor "Valentina Maccatrozzo" ;
    swrc:pages "391-398" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-642-35173-0_28>,
        <http://iswc2012.semanticweb.org/sites/default/files/76500382.pdf> ;
    swrc:year "2012" ;
    foaf:maker <http://data.semanticweb.org/person/valentina-maccatrozzo> .

<http://data.semanticweb.org/conference/iswc/2012/proceedings-1/paper-04> a swrc:InProceedings ;
    rdfs:label "Reconstructing Provenance" ;
    swc:isPartOf <http://data.semanticweb.org/conference/iswc/2012/proceedings-1> ;
