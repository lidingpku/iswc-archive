<http://data.semanticweb.org/conference/iswc/2012/proceedings-2/paper-37> a swrc:InProceedings ;
    rdfs:label "An Evidence-based Verification Approach to Extract Entities and Relations for Knowledge Base Population" ;
    swc:isPartOf <http://data.semanticweb.org/conference/iswc/2012/proceedings-2> ;
    dc:title "An Evidence-based Verification Approach to Extract Entities and Relations for Knowledge Base Population" ;
