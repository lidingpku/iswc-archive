    swrc:url <http://ceur-ws.org/Vol-914/paper_11.pdf> ;
    swrc:year "2012" ;
    foaf:maker <http://data.semanticweb.org/person/patrick-siehndel>,
        <http://data.semanticweb.org/person/ricardo-kawase> .

