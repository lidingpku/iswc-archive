    swrc:listAuthor "Kristian Slabbekoorn,Laura Hollink and Geert-Jan Houben" ;
    swrc:listKeyword "Domain detection,Information retrieval,Ontology matching" ;
    swrc:pages "542-558" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-642-35176-1_34>,
