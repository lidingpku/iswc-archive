    swrc:listAuthor "Cory A. Henson,Krishnaprasad Thirunarayan and Amit P. Sheth" ;
    swrc:listKeyword "Mobile Device, Resource-Constrained Environments, Semantic Sensor Web, Sensor Data,Machine Perception" ;
