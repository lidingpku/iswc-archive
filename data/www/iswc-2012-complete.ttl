    swrc:listAuthor "Olaf Görlitz,Matthias Thimm and Steffen Staab" ;
    swrc:listKeyword "Benchmark,Distributed Query Processing,Linked Data,RDF,SPARQL" ;
    swrc:pages "116-132" ;
