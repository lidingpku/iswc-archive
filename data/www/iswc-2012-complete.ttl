    swrc:link_publisher <http://dx.doi.org/10.1007/978-3-642-35173-0_7> ;
    swrc:listAuthor "Takahiro Kawamura and Akihiko Ohsuga" ;
    swrc:listKeyword "AR,Field,LOD,Plant,Sensor" ;
    swrc:pages "98-113" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-642-35173-0_7>,
        <http://iswc2012.semanticweb.org/sites/default/files/76500097.pdf> ;
