        <http://data.semanticweb.org/person/takeshi-imai>,
        <http://data.semanticweb.org/person/yuki-yamagata> ;
    swrc:category "Poster/Demo Paper" ;
    swrc:link_open_access <http://ceur-ws.org/Vol-914/paper_51.pdf> ;
    swrc:listAuthor "Kouji Kozaki,Hiroko Kou,Yuki Yamagata,Takeshi Imai,Kazuhiko Ohe and Riichiro Mizoguchi" ;
    swrc:listKeyword "causal chain,disease ontology,ontology visualization" ;
    swrc:url <http://ceur-ws.org/Vol-914/paper_51.pdf> ;
