    swrc:listAuthor "Giuseppe Pirrò and Jérôme Euzenat" ;
    swrc:listKeyword "semantic web" ;
    swrc:pages "615-630" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-642-17746-0_39>,
        <http://iswc2010.semanticweb.org/pdf/57.pdf> ;
