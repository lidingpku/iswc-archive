    swrc:link_publisher <http://ceur-ws.org/Vol-658/paper524.pdf> ;
    swrc:listAuthor "Daniel Izquierdo,Maria-Esther Vidal and Blai Bonet" ;
    swrc:url <http://ceur-ws.org/Vol-658/paper524.pdf>,
