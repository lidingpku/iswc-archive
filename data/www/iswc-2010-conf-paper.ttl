    swrc:listKeyword "semantic web" ;
    swrc:pages "356-369" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-642-17749-1_28>,
        <http://iswc2010.semanticweb.org/pdf/903.pdf> ;
    swrc:year "2010" ;
    foaf:maker <http://data.semanticweb.org/person/m-c-schraefel> .

