    swrc:link_publisher <http://dx.doi.org/10.1007/978-3-642-17749-1_18> ;
    swrc:listAuthor "Guo-Qiang Zhang and Olivier Bodenreider" ;
    swrc:listKeyword "semantic web" ;
    swrc:pages "273-288" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-642-17749-1_18>,
        <http://iswc2010.semanticweb.org/pdf/407.pdf> ;
    swrc:year "2010" ;
