    swrc:listKeyword "semantic web" ;
    swrc:pages "631-646" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-642-17746-0_40>,
        <http://iswc2010.semanticweb.org/pdf/278.pdf> ;
