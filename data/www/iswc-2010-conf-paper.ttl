    swrc:listKeyword "semantic web" ;
    swrc:pages "730-746" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-642-17746-0_46>,
        <http://iswc2010.semanticweb.org/pdf/243.pdf> ;
    swrc:year "2010" ;
    foaf:maker <http://data.semanticweb.org/person/rudi-studer>,
