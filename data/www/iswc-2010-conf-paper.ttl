    swrc:listKeyword "semantic web" ;
    swrc:pages "321-336" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-642-17746-0_21>,
        <http://iswc2010.semanticweb.org/pdf/169.pdf> ;
    swrc:year "2010" ;
    foaf:maker <http://data.semanticweb.org/person/hans-joerg-happel> .

