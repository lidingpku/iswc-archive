    swrc:listAuthor "Elena Beisswanger" ;
    swrc:listKeyword "ontology alignment,relation extraction,semantic web,wikipedia" ;
    swrc:pages "289-296" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-642-17749-1_19>,
        <http://iswc2010.semanticweb.org/pdf/440.pdf> ;
    swrc:year "2010" ;
    foaf:maker <http://data.semanticweb.org/person/elena-beisswanger> .

