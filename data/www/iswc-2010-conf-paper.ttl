    swrc:listAuthor "Tamar Domany,Abigail Tarem and David Ben-David" ;
    swrc:listKeyword "Classification,NeON,RDF,RelationalOWL,Semantic Techniques,modeling" ;
    swrc:url <http://ceur-ws.org/Vol-658/paper476.pdf>,
        <http://iswc2010.semanticweb.org/pdf/476.pdf> ;
    swrc:year "2010" ;
    foaf:maker <http://data.semanticweb.org/person/abigail-tarem>,
