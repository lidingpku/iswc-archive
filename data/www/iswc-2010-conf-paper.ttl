    swrc:listKeyword "FOAF,linked data,machine learning" ;
    swrc:url <http://ceur-ws.org/Vol-658/paper514.pdf>,
        <http://iswc2010.semanticweb.org/pdf/514.pdf> ;
    swrc:year "2010" ;
    foaf:maker <http://data.semanticweb.org/person/jennifer-sleeman>,
