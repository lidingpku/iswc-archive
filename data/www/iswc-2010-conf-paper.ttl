    swrc:listKeyword "semantic web" ;
    swrc:pages "713-729" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-642-17746-0_45>,
        <http://iswc2010.semanticweb.org/pdf/304.pdf> ;
    swrc:year "2010" ;
