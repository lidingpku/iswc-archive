    swrc:link_publisher <http://ceur-ws.org/Vol-658/paper506.pdf> ;
    swrc:listAuthor "Andreas Wagner,Sebastian Speiser and Andreas Harth" ;
    swrc:url <http://ceur-ws.org/Vol-658/paper506.pdf>,
        <http://iswc2010.semanticweb.org/pdf/506.pdf> ;
    swrc:year "2010" ;
    foaf:maker <http://data.semanticweb.org/person/andreas-harth>,
        <http://data.semanticweb.org/person/andreas-wagner>,
        <http://data.semanticweb.org/person/sebastian-speiser> .

<http://data.semanticweb.org/conference/iswc/2010/poster-demo-proceedings/paper-37> a swrc:InProceedings ;
    rdfs:label "Silk - Generating RDF Links while publishing or consuming Linked Data" ;
    swc:isPartOf <http://data.semanticweb.org/conference/iswc/2010/poster-demo-proceedings> ;
    dc:title "Silk - Generating RDF Links while publishing or consuming Linked Data" ;
    dcterms:title "Silk - Generating RDF Links while publishing or consuming Linked Data" ;
    bibo:authorList <http://data.semanticweb.org/conference/iswc/2010/poster-demo-proceedings/paper-37/author_list> ;
    swrc:abstract "The central idea of the Web of Data is to interlink data items using RDF links. However, in practice most data sources are not sufficiently interlinked with related data sources. The Silk Link Discovery Framework addresses this problem by providing tools to generate links between data items based on user-provided link specifications. It can be used by data publishers to generate links between data sets as well as by Linked Data consumers to augment Web data with additional RDF links. In this poster we present the Silk Link Discovery Framework and report on two usage examples in which we employed Silk to generate links between two data sets about movies as well as to find duplicate persons in a stream of data items that is crawled from the Web." ;
    swrc:author <http://data.semanticweb.org/person/anja-jentzsch>,
        <http://data.semanticweb.org/person/christian-bizer>,
        <http://data.semanticweb.org/person/robert-isele> ;
