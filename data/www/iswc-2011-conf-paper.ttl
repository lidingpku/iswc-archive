    swrc:listAuthor "Elena Simperl,Roberta Cuel,Markus Rohde and Carl Goodman" ;
    swrc:listKeyword "game mechanics,incentives,motivators,semantic content authoring,user experience design" ;
    swrc:url <http://www.insemtives.eu/iswc2011-tutorial/> ;
    swrc:year "2011" ;
    foaf:maker <http://data.semanticweb.org/person/carl-goodman>,
