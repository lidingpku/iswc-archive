    swrc:url <http://iswc2011.semanticweb.org/fileadmin/iswc/Papers/PostersDemos/iswc11pd_submission_30.pdf> ;
    swrc:year "2011" ;
    foaf:maker <http://data.semanticweb.org/person/agnieszka-lawrynowicz>,
