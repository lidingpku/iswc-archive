    swrc:listKeyword "ontology debugging,ontology development,ontology engineering,ontology evolution,ontology mapping" ;
    swrc:url <http://iswc2011.semanticweb.org/tutorials/debugging-ontologies-and-mappings-in-ontology-networks/> ;
