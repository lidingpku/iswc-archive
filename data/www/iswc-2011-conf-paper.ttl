    swrc:listAuthor "Yang Yu and Jeff Heflin" ;
    swrc:listKeyword "Semantic Web data quality,abnormal data in RDF graphs,value-clustered graph functional dependency" ;
    swrc:pages "794-809" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-642-25073-6_50>,
