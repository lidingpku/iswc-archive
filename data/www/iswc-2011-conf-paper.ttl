<http://data.semanticweb.org/conference/iswc/2011/proceedings-2/paper-19> a swrc:InProceedings ;
    rdfs:label "DC Proposal: Towards Linked Data Assessment and Linking Temporal Facts" ;
    swc:isPartOf <http://data.semanticweb.org/conference/iswc/2011/proceedings-2> ;
    dc:title "DC Proposal: Towards Linked Data Assessment and Linking Temporal Facts" ;
