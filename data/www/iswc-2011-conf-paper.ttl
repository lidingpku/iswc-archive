    swrc:listAuthor "Vit Novacek,Tudor Groza and Siegfried Handschuh" ;
    swrc:listKeyword "distributional semantics,emergent semantics,knowledge acquisition,knowledge discovery,knowledge exploration" ;
    swrc:url <http://iswc2011.semanticweb.org/fileadmin/iswc/Papers/PostersDemos/iswc11pd_submission_50.pdf> ;
