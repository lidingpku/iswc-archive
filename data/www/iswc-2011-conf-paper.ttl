    swrc:url <http://iswc2011.semanticweb.org/fileadmin/iswc/Papers/PostersDemos/iswc11pd_submission_34.pdf> ;
    swrc:year "2011" ;
    foaf:maker <http://data.semanticweb.org/person/lihua-zhao>,
