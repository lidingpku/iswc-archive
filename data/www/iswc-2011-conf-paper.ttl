    swrc:listKeyword "Query Optimizations,RDB2RDF,RDF,Relational Databases,SPARQL" ;
    swrc:url <http://iswc2011.semanticweb.org/fileadmin/iswc/Papers/PostersDemos/iswc11pd_submission_94.pdf> ;
    swrc:year "2011" ;
