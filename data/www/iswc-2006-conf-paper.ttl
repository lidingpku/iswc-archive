    swrc:pages "723-735" ;
    swrc:url <http://dx.doi.org/10.1007/11926078_52>,
        <http://iswc2006.semanticweb.org/items/Hage2006yq.pdf> ;
    swrc:year "2006" ;
    foaf:maker <http://data.semanticweb.org/person/guus-schreiber>,
