    swrc:pages "399-413" ;
    swrc:url <http://dx.doi.org/10.1007/11926078_29>,
        <http://iswc2006.semanticweb.org/items/Liarou2006wd.pdf> ;
    swrc:year "2006" ;
    foaf:maker <http://data.semanticweb.org/person/erietta-liarou>,
