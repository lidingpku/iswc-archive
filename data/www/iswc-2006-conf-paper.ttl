    swrc:pages "573-582" ;
    swrc:url <http://dx.doi.org/10.1007/11926078_41> ;
    swrc:year "2006" ;
    foaf:maker <http://data.semanticweb.org/person/murat-osman-uenalir>,
        <http://data.semanticweb.org/person/oevuenc-oeztuerk>,
        <http://data.semanticweb.org/person/tugba-oezacar> .

<http://data.semanticweb.org/conference/iswc/2006/proceedings/paper-64> a swrc:InProceedings ;
