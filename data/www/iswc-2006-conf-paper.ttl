    swrc:listAuthor "Taowei David Wang,Bijan Parsia and James A. Hendler" ;
    swrc:pages "682-694" ;
    swrc:url <http://dx.doi.org/10.1007/11926078_49>,
        <http://iswc2006.semanticweb.org/items/Wang2006ly.pdf> ;
    swrc:year "2006" ;
    foaf:maker <http://data.semanticweb.org/person/bijan-parsia>,
