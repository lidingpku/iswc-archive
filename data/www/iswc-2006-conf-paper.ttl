    swrc:link_publisher <http://dx.doi.org/10.1007/11926078_51> ;
    swrc:listAuthor "Christopher A. Welty and J. William Murdock" ;
    swrc:pages "709-722" ;
    swrc:url <http://dx.doi.org/10.1007/11926078_51>,
        <http://iswc2006.semanticweb.org/items/Welty2006tw.pdf> ;
    swrc:year "2006" ;
    foaf:maker <http://data.semanticweb.org/person/christopher-a-welty>,
