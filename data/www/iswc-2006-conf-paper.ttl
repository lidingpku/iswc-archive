    swrc:listAuthor "Natalya Fridman Noy,Abhita Chugh,William Liu and Mark A. Musen" ;
    swrc:pages "544-558" ;
    swrc:url <http://dx.doi.org/10.1007/11926078_39>,
        <http://iswc2006.semanticweb.org/items/Noy2006fj.pdf> ;
    swrc:year "2006" ;
