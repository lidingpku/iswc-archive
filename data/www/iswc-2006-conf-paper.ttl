    swrc:link_publisher <http://dx.doi.org/10.1007/11926078_23> ;
    swrc:listAuthor "Carlos A. Hurtado,Alexandra Poulovassilis and Peter T. Wood" ;
    swrc:pages "314-328" ;
    swrc:url <http://dx.doi.org/10.1007/11926078_23>,
        <http://iswc2006.semanticweb.org/items/Hurtado2006rt.pdf> ;
    swrc:year "2006" ;
    foaf:maker <http://data.semanticweb.org/person/alexandra-poulovassilis>,
