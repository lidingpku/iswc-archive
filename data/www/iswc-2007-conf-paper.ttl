    swrc:link_publisher <http://dx.doi.org/10.1007/978-3-540-76298-0_25> ;
    swrc:listAuthor "David Martin,Massimo Paolucci and Matthias Wagner" ;
    swrc:pages "340-352" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-540-76298-0_25>,
        <http://iswc2007.semanticweb.org/papers/337.pdf> ;
    swrc:year "2007" ;
    foaf:maker <http://data.semanticweb.org/person/david-martin>,
