    swrc:url <http://citeseerx.ist.psu.edu/viewdoc/download?doi=10.1.1.104.4481&rep=rep1&type=pdf>,
        <http://dx.doi.org/10.1007/978-3-540-76298-0_74> ;
    swrc:year "2007" ;
    foaf:maker <http://data.semanticweb.org/person/azzurra-ragone> .

