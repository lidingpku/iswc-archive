    swrc:listAuthor "Renate A. Schmidt and Dmitry Tishkovsky" ;
    swrc:pages "438-451" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-540-76298-0_32>,
        <http://iswc2007.semanticweb.org/papers/435.pdf> ;
    swrc:year "2007" ;
