    swrc:listAuthor "Hyun Namgoong and Hong-Gee Kim" ;
    swrc:pages "353-366" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-540-76298-0_26>,
        <http://iswc2007.semanticweb.org/papers/351.pdf> ;
    swrc:year "2007" ;
    foaf:maker <http://data.semanticweb.org/person/hong-gee-kim>,
