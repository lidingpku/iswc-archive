    swrc:url <http://dx.doi.org/10.1007/978-3-540-76298-0_36>,
        <http://iswc2007.semanticweb.org/papers/491.pdf> ;
    swrc:year "2007" ;
    foaf:maker <http://data.semanticweb.org/person/he-tan>,
        <http://data.semanticweb.org/person/patrick-lambrix> .

<http://data.semanticweb.org/conference/iswc-aswc/2007/proceedings/paper-08> a swrc:InProceedings ;
    rdfs:label "An Event-Based Approach for Semantic Metadata Interoperability" ;
    swc:isPartOf <http://data.semanticweb.org/conference/iswc-aswc/2007/proceedings> ;
