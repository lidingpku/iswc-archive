        <http://iswc2007.semanticweb.org/papers/603.pdf> ;
    swrc:year "2007" ;
    foaf:maker <http://data.semanticweb.org/person/takashi-yamauchi> .

<http://data.semanticweb.org/conference/iswc-aswc/2007/poster-demo-proceedings/paper-07> a swrc:InProceedings ;
    rdfs:label "A Statistical Approach for Semantic Search using Heterogenous Semantics on the Web" ;
    swc:isPartOf <http://data.semanticweb.org/conference/iswc-aswc/2007/poster-demo-proceedings> ;
    dc:title "A Statistical Approach for Semantic Search using Heterogenous Semantics on the Web" ;
