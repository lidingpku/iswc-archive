    swrc:listAuthor "Michael Stollberg,Martin Hepp and Jörg Hoffmann" ;
    swrc:pages "480-493" ;
    swrc:url <http://dx.doi.org/10.1007/978-3-540-76298-0_35>,
        <http://iswc2007.semanticweb.org/papers/477.pdf> ;
