    swrc:title "Integrating Uncertainty Into Ontology Mapping" ;
    swrc:url <http://citeseerx.ist.psu.edu/viewdoc/download?doi=10.1.1.107.4940&rep=rep1&type=pdf>,
        <http://data.semanticweb.org/pdfs/iswc-aswc/2007/ISWC2007_DC_Wang.pdf>,
        <http://dx.doi.org/10.1007/978-3-540-76298-0_78> ;
    swrc:year "2007" ;
    swrc_ext:authorList [ rdf:_1 <http://data.semanticweb.org/person/ying-wang> ] ;
