<http://data.semanticweb.org/conference/iswc-aswc/2007/tracks/posters-demos/papers/375> a swrc:InProceedings ;
    rdfs:label "Expertise Object Search" ;
    swc:isPartOf <http://data.semanticweb.org/conference/iswc-aswc/2007/tracks/posters-demos/proceedings> ;
    swc:relatedToEvent <http://data.semanticweb.org/conference/iswc-aswc/2007/tracks/posters-demos/talks/375> ;
    dc:creator <http://data.semanticweb.org/person/jie-tang> ;
    dc:title "Expertise Object Search" ;
    swrc:abstract "This paper addresses the issue of expertise object search. In Semantic Web, information is described as concepts and relations between the concepts. Previous document-level information search can unfortunately lead to highly inaccurate results in answering semantic-oriented queries. Previous work on semantic search is targeted at finding the most relevant objects. In this paper, we investigate several models for expertise object search, namely local language model, propagation based model, and a unified model." ;
    swrc:author <http://data.semanticweb.org/person/jie-tang> ;
    swrc:month "November" ;
    swrc:pages "107--108" ;
    swrc:title "Expertise Object Search" ;
