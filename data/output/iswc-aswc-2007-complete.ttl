    swc:isPartOf <http://data.semanticweb.org/conference/iswc-aswc/2007/tracks/posters-demos/proceedings> ;
    swc:relatedToEvent <http://data.semanticweb.org/conference/iswc-aswc/2007/tracks/posters-demos/talks/401> ;
