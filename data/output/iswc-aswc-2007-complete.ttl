    :maker <http://data.semanticweb.org/person/jie-tang> .

<http://data.semanticweb.org/conference/iswc-aswc/2007/tracks/posters-demos/papers/394> a swrc:InProceedings ;
    rdfs:label "Semantic Web Architecture" ;
    swc:isPartOf <http://data.semanticweb.org/conference/iswc-aswc/2007/tracks/posters-demos/proceedings> ;
    swc:relatedToEvent <http://data.semanticweb.org/conference/iswc-aswc/2007/tracks/posters-demos/talks/394> ;
    dc:creator <http://data.semanticweb.org/person/ila-nivas> ;
    dc:title "Semantic Web Architecture" ;
    swrc:abstract "In this poster the semantic web architecture for 'open market' concept is described. 'Open market' is a commercial application of semantic technologies. The semantic web architecture elements have been categorized as: web service provider, web service requester and infrastructure elements. This architecture addresses the standard ontology discovery, semantic web service discovery, availability of semantic web content with embedded normative metadata and semantic web service integration automation requirements of the semantic web." ;
    swrc:author <http://data.semanticweb.org/person/ila-nivas> ;
    swrc:month "November" ;
    swrc:pages "77--78" ;
    swrc:title "Semantic Web Architecture" ;
