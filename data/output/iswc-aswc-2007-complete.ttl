    swrc:month "November" ;
    swrc:pages "15--28" ;
    swrc:title "Automatically Composing Data Workflows with Relational Descriptions and Shim Services" ;
    swrc:url <http://data.semanticweb.org/pdfs/iswc-aswc/2007/ISWC2007_RT_Ambite.pdf>,
