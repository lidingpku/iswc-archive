    swrc:title "A Statistical Approach for Semantic Search using Heterogenous Semantics on the Web" ;
    swrc:year "2007" ;
    swrc_ext:authorList [ rdf:_1 <http://data.semanticweb.org/person/rui-huang> ;
            rdf:_2 <http://data.semanticweb.org/person/zhongzhi-shi> ] ;
    owl:sameAs <http://revyu.com/things/iswc-aswc-2007-posters-demos-paper-374-a-statistical> ;
    :maker <http://data.semanticweb.org/person/rui-huang>,
        <http://data.semanticweb.org/person/zhongzhi-shi> .

<http://data.semanticweb.org/conference/iswc-aswc/2007/tracks/posters-demos/papers/384> a swrc:InProceedings ;
