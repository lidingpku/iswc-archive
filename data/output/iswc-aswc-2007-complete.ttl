    swrc:month "November" ;
    swrc:pages "910--914" ;
    swrc:title "From Texts to Structured Documents: The Case of Health Practice Guidelines" ;
    swrc:url <http://data.semanticweb.org/pdfs/iswc-aswc/2007/ISWC2007_DC_Bouffier.pdf>,
        <http://dx.doi.org/10.1007/978-3-540-76298-0_69>,
        <http://iswc2007.semanticweb.org/papers/910.pdf> ;
    swrc:year "2007" ;
    swrc_ext:authorList [ rdf:_1 <http://data.semanticweb.org/person/amanda-bouffier> ] ;
